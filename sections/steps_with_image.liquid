<div class="padding-global padding-section-large">
          <div class="container-large">
            <div class="content_grid">{% if section.settings.image_image %}
{{ section.settings.image_image | image_url: width: 1000 | image_tag: loading: 'lazy', class: 'content_image' }}
{% else %}
<img loading="lazy" alt="" src="{{ 'image.svg' | asset_url }}" class="content_image">
{% endif %}
